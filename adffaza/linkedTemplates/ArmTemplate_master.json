{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adffaza"
		},
		"ADLS_faza_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_faza'"
		},
		"AzureSQL_faza_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQL_faza'"
		},
		"SQL_LAPTOP_0DOCT8SG_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_LAPTOP_0DOCT8SG'"
		},
		"SQL_faza_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_faza'"
		},
		"ADLS_hanifstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_hanifstorage'"
		},
		"SQL_hanif_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_hanif'"
