						"value": "[parameters('AzureSQL_faza_properties_typeProperties_server')]"
					},
					"AzureSQL_faza_properties_typeProperties_database": {
						"value": "[parameters('AzureSQL_faza_properties_typeProperties_database')]"
					},
					"AzureSQL_faza_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQL_faza_properties_typeProperties_userName')]"
					},
					"SQL_faza_properties_typeProperties_server": {
						"value": "[parameters('SQL_faza_properties_typeProperties_server')]"
					},
					"SQL_faza_properties_typeProperties_database": {
						"value": "[parameters('SQL_faza_properties_typeProperties_database')]"
					},
					"SQL_faza_properties_typeProperties_userName": {
						"value": "[parameters('SQL_faza_properties_typeProperties_userName')]"
					},
					"HTTP_solliancepublicdata_Customer_properties_typeProperties_url": {
						"value": "[parameters('HTTP_solliancepublicdata_Customer_properties_typeProperties_url')]"
					},
					"ADLS_hanifstorage_properties_typeProperties_url": {
						"value": "[parameters('ADLS_hanifstorage_properties_typeProperties_url')]"
					},
					"SQL_hanif_properties_typeProperties_server": {
						"value": "[parameters('SQL_hanif_properties_typeProperties_server')]"
					},
					"SQL_hanif_properties_typeProperties_database": {
						"value": "[parameters('SQL_hanif_properties_typeProperties_database')]"
					},
					"SQL_hanif_properties_typeProperties_userName": {
						"value": "[parameters('SQL_hanif_properties_typeProperties_userName')]"
					},
					"SQL_LAPTOP_0DOCT8SG_properties_typeProperties_server": {
						"value": "[parameters('SQL_LAPTOP_0DOCT8SG_properties_typeProperties_server')]"
					},
					"SQL_LAPTOP_0DOCT8SG_properties_typeProperties_database": {
						"value": "[parameters('SQL_LAPTOP_0DOCT8SG_properties_typeProperties_database')]"
					},
					"SQL_LAPTOP_0DOCT8SG_properties_typeProperties_userName": {
						"value": "[parameters('SQL_LAPTOP_0DOCT8SG_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adffaza_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adffaza_ArmTemplate_0')]"
			]
		}
