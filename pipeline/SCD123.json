				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "WITH NumberedEmployees AS (\n    SELECT *,\n           ROW_NUMBER() OVER (PARTITION BY EmployeeID ORDER BY EndDate DESC) AS row_num\n    FROM Employee\n),\nPreviousPosition AS (\n    SELECT EmployeeID, \n           PreviousPosition\n    FROM (\n        SELECT EmployeeID, \n               Position as PreviousPosition,\n               ROW_NUMBER() OVER (PARTITION BY EmployeeID ORDER BY EndDate DESC) AS row_num\n        FROM Employee\n    ) subpp\n    WHERE subpp.row_num = 2\n),\nStartDate AS (\n    SELECT EmployeeID, StartDate\n    FROM (\n        SELECT EmployeeID, Position, StartDate,\n               ROW_NUMBER() OVER (PARTITION BY EmployeeID ORDER BY EndDate ASC) AS row_num\n        FROM Employee\n    ) subsd\n    WHERE subsd.row_num = 1\n)\nSELECT e.EmployeeLogID,\n       e.EmployeeID,\n       e.Name,\n       e.Position,\n       COALESCE(pp.PreviousPosition, e.Position) AS PreviousPosition,\n       sd.StartDate,\n       e.EndDate\nFROM NumberedEmployees e\nLEFT JOIN StartDate sd ON e.EmployeeID = sd.EmployeeID\nLEFT JOIN PreviousPosition pp ON sd.EmployeeID = pp.EmployeeID\nWHERE e.row_num = 1;",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
