{
	"name": "MongoDb to synapse",
	"properties": {
		"activities": [
			{
				"name": "documents",
				"type": "Copy",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbV2Source",
						"batchSize": 100
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "MongoDb_Dataset",
						"type": "DatasetReference",
						"parameters": {
							"collection_name": "documents"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "JSON_ADLS_faza",
						"type": "DatasetReference",
						"parameters": {
							"container_name": "fazatest",
							"folder_name": {
								"value": "@concat('raw/', 'documents/', formatDateTime(utcnow(), 'yyyy-MM-dd'))",
								"type": "Expression"
							},
							"file_name": "documents.json"
						}
					}
				]
			},
			{
				"name": "Notebook1",
				"type": "DatabricksNotebook",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [
					{
						"activity": "documents",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/kelompok_4@chococwefdit.onmicrosoft.com/Faza/MongoDb to Synapse (parquet)/Soal 1"
				},
				"linkedServiceName": {
					"referenceName": "Databricks_Faza",
					"type": "LinkedServiceReference"
				}
			}
		],
		"annotations": []
	}
}